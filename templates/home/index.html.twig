{% extends 'base.html.twig' %}

{% block title %}Home{% endblock %}

{% block body %}
<h1 class="text-secondary bg-primary d-flex justify-content-center">Wild Series</h1>
    <div class="container">
        <div class="row">
            {% for program in programs %}
            <div class="col-4 mt-4 mb-4">
                <div class="card">
                    <img src="{{ upload_program_dir }}{{ program.poster }}" alt="got">
                    <h2>{{ program.title }}</h2>
                </div>
            </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}
